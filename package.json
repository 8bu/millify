{
  "name": "millify",
  "version": "2.0.1",
  "description": "Human-readable large numbers for Node.js",
  "main": "src/index.js",
  "bin": {
    "millify": "src/cli.js"
  },
  "scripts": {
    "test": "ava",
    "lint": "eslint --fix src",
    "standard": "prettier-standard 'src/**/*.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npm run standard",
      "npm run lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/izolate/millify.git"
  },
  "keywords": [
    "numbers",
    "format",
    "human",
    "pretty",
    "million",
    "billion",
    "trillion"
  ],
  "author": "izolate <yoshtalwar@gmail.com> (http://izolate.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/izolate/millify/issues"
  },
  "homepage": "https://github.com/izolate/millify#readme",
  "devDependencies": {
    "ava": "^0.21.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "prettier-standard": "^6.0.0"
  },
  "dependencies": {}
}
